entity Meal {
	when ZonedDateTime
	breakfast Recipe,
	lunch Recipe,
	dinner Recipe
}

entity Recipe {
	name String,
}

relationship OneToOne {
  Meal{breakfast} to Recipe,
  Meal{lunch} to Recipe,
  Meal{dinner} to Recipe,
}

entity Category {
	name String
}

relationship ManyToMany {
	Recipe{tag(name)} to Tag{recipe}
}
enum MealTime {
    BREAKFAST, LUNCH, DINNER
}
